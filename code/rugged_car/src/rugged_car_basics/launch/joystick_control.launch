<launch>
  <!-- Listen to messages from joysticks -->
  <node pkg="joy" name="joy_node" type="joy_node"/>
  
  <node pkg="rugged_car_basics" name="joy_control" type="joy_control" output="screen">
    <rosparam command="load" file="$(find rugged_car_basics)/params.yaml"/>
  </node>
  
   <!-- Start the stanley control testing node -->
  <node pkg="rugged_car_basics" type="stanley_control" name="stanley_control" output="screen">
    <rosparam command="load" file="$(find rugged_car_basics)/params.yaml"/>
  </node>

  <!-- Start the communication node -->
  <node pkg="rugged_car_basics" type="mbot_linux_serial" name="mbot_linux_serial" respawn="true"/>
  
  <!-- 在rviz中显示-->
  <include file="$(find realsense2_camera)/launch/rs_t265.launch"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find realsense2_camera)/rviz/tracking.rviz" required="true" />  
  
</launch>
